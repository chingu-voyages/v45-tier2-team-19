{
  "name": "fireball",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|css)\"",
    "check-format": "prettier --ignore-path .gitignore --list-different \"**/*.+(js|jsx|json|css)\"",
    "preview": "vite preview",
    "validate": "npm run lint && npm run test && npm run build",
    "setup": "npm install && npm run validate",
    "prepare": "cd .. && husky install fireball/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.7",
    "@radix-ui/colors": "^2.1.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.1.2",
    "chart.js": "^4.3.3",
    "d3-array": "^3.2.4",
    "d3-fetch": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-geo": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-table": "^7.8.0",
    "react-virtualized": "^9.22.5",
    "react-virtuoso": "^4.5.0",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.5",
    "vitest": "^0.34.3"
  },
  "lint-staged": {
    "\"**/*.+(js|jsx|json|css)\"": "npm run format"
  }
}
